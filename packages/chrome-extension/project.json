{
  "name": "chrome-extension",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/chrome-extension/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "compiler": "babel",
        "outputPath": "dist/packages/chrome-extension",
        "baseHref": "/",
        "main": "packages/chrome-extension/src/index.ts",
        "tsConfig": "packages/chrome-extension/tsconfig.app.json",
        "assets": [
          "packages/chrome-extension/src/assets",
          "packages/chrome-extension/src/styles.css"
        ],
        "scripts": [],
        "isolatedConfig": true,
        "webpackConfig": "packages/chrome-extension/webpack.config.js"
      },
      "configurations": {
        "development": {},
        "production": {
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false,
          "minimize": true
        }
      }
    },
    "serve": {
      "executor": "@nx/js:node",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "chrome-extension:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "chrome-extension:build:development"
        },
        "production": {
          "buildTarget": "chrome-extension:build:production"
        }
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/chrome-extension/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/chrome-extension/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "release": {
      "executor": "nx:run-commands",
      "outputs": [],
      "options": {
        "command": "npx semantic-release-plus --extends ./packages/chrome-extension/release.config.js"
      }
    }
  },
  "tags": []
}
