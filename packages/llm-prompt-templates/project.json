{
  "name": "llm-prompt-templates",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/llm-prompt-templates/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/llm-prompt-templates",
        "main": "packages/llm-prompt-templates/src/index.ts",
        "tsConfig": "packages/llm-prompt-templates/tsconfig.lib.json",
        "assets": ["packages/llm-prompt-templates/*.md"],
        "project": "packages/llm-prompt-templates/package.json"
      }
    },
    "publish": {
      "command": "node tools/scripts/publish.mjs llm-prompt-templates {args.ver} {args.tag}",
      "dependsOn": ["build"]
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/llm-prompt-templates/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/llm-prompt-templates/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    }
  },
  "tags": []
}
